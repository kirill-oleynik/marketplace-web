{
  "scripts": {
    "dev": "yarn run clean && node server.js",
    "lint": "eslint app --ext .js",
    "test": "jest",
    "start": "pm2 kill && pm2 start web.json",
    "build": "yarn run clean && next build app && yarn run cp-css && yarn run gzip",
    "clean": "rm -rf build",
    "cp-css": "cp build/styles.css app/static/css/styles.css",
    "gzip": "yarn run gzip-css && yarn run gzip-fonts",
    "gzip-css": "gzip -rkf9 app/static/css",
    "gzip-fonts": "gzip -rkf9 app/static/fonts"
  },
